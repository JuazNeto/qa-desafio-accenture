						"exec": [
							"let responseData = pm.response.json(); pm.environment.set(\"userID\", responseData.userID);\r",
							"\r",
							"pm.test(\"User ID was generated successfully.\", function () {\r",
							"    let jsonData = pm.response.json();\r",
